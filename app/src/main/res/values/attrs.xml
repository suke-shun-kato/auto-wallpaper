<resources>
    <!-- callbackUrlが2つのライブラリで使われているので、ここで宣言している -->
    <attr name="callbackUrl" format="string" />

    <declare-styleable name="SelectDirPreference">
        <attr name="dialogTitle" format="string" />
    </declare-styleable>
    <declare-styleable name="TwitterOAuthPreference">
        <attr name="textCantAccessAuthPage" format="string" />
        <attr name="textOauthSuccess" format="string" />
        <attr name="textOauthFailed" format="string" />
        <attr name="callbackUrl" />
    </declare-styleable>
    <declare-styleable name="InstagramOAuthPreference">
        <attr name="callbackUrl" />
        <attr name="clientID" format="string"/>
        <attr name="clientSecret" format="string"/>
        <attr name="summaryDone" format="string"/>
        <attr name="summaryNotYet" format="string"/>
    </declare-styleable>
    <declare-styleable name="ResetBtnPreference">
        <attr name="textDialogMsg" format="string" />
        <attr name="textDialogTitle" format="string" />
        <attr name="textResult" format="string" />
    </declare-styleable>
    <declare-styleable name="TimePreference">
        <!--<attr name="textDialogTitle" format="string" />-->
    </declare-styleable>
</resources>